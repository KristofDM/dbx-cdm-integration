{
  "jsonSchemaSemanticVersion": "1.5.0",
  "imports": [
    { "corpusPath": "cdm:/foundations.4.3.cdm.json" }
  ],
  "definitions": [
    {
      "entityName": "KYCCheck",
      "exhibitsTraits": [
        {
          "traitReference": "is.CDM.entityVersion",
          "arguments": [{ "name": "versionNumber", "value": "1.0" }]
        },
        {
          "traitReference": "is.extension.custom",
          "arguments": [
            { "name": "organizationName", "value": "Contoso Banking Group" },
            { "name": "entityPurpose", "value": "Custom entity for KYC (Know Your Customer) compliance tracking. Not part of standard CDM â€” created for Belgian AML/CFT regulatory requirements." }
          ]
        }
      ],
      "description": "Custom entity for tracking KYC (Know Your Customer) verification checks on contacts. Created for AML (Anti-Money Laundering) and CFT (Counter-Financing of Terrorism) compliance. Linked to the CDM Contact entity.",
      "hasAttributes": [
        {
          "name": "kycCheckId",
          "dataType": "entityId",
          "isNullable": false,
          "isPrimaryKey": true,
          "description": "Unique identifier for the KYC check record"
        },
        {
          "name": "contactId",
          "dataType": "entityId",
          "isNullable": false,
          "description": "Foreign key to the Contact entity being verified"
        },
        {
          "name": "createdOn",
          "dataType": "dateTime",
          "isNullable": true,
          "description": "Date and time when the record was created"
        },
        {
          "name": "modifiedOn",
          "dataType": "dateTime",
          "isNullable": true,
          "description": "Date and time when the record was last modified"
        },
        {
          "name": "statecode",
          "dataType": "listLookup",
          "isNullable": false,
          "description": "Status of the KYC check record. Values: 0=Active, 1=Inactive",
          "optionSet": {
            "values": [
              { "value": 0, "displayText": "Active" },
              { "value": 1, "displayText": "Inactive" }
            ]
          }
        },
        {
          "name": "statecode_display",
          "dataType": "string",
          "isNullable": true,
          "description": "Display text for statecode"
        },
        {
          "name": "statuscode",
          "dataType": "listLookup",
          "isNullable": true,
          "description": "Reason for the status",
          "optionSet": {
            "values": [
              { "value": 1, "displayText": "Active" },
              { "value": 2, "displayText": "Inactive" }
            ]
          }
        },
        {
          "name": "statuscode_display",
          "dataType": "string",
          "isNullable": true,
          "description": "Display text for statuscode"
        },
        {
          "name": "checkType",
          "dataType": "listLookup",
          "isNullable": true,
          "description": "Type of KYC verification performed",
          "optionSet": {
            "values": [
              { "value": 0, "displayText": "ID Verification" },
              { "value": 1, "displayText": "Address Proof" },
              { "value": 2, "displayText": "Income Verification" },
              { "value": 3, "displayText": "PEP Check" },
              { "value": 4, "displayText": "Sanctions Screening" }
            ]
          }
        },
        {
          "name": "checkType_display",
          "dataType": "string",
          "isNullable": true,
          "description": "Display text for checkType"
        },
        {
          "name": "checkDate",
          "dataType": "dateTime",
          "isNullable": true,
          "description": "Date and time when the KYC check was performed"
        },
        {
          "name": "expiryDate",
          "dataType": "dateTime",
          "isNullable": true,
          "description": "Date when the KYC check result expires and needs renewal"
        },
        {
          "name": "checkResult",
          "dataType": "listLookup",
          "isNullable": true,
          "description": "Result of the KYC check",
          "optionSet": {
            "values": [
              { "value": 0, "displayText": "Pass" },
              { "value": 1, "displayText": "Fail" },
              { "value": 2, "displayText": "Pending" },
              { "value": 3, "displayText": "Expired" }
            ]
          }
        },
        {
          "name": "checkResult_display",
          "dataType": "string",
          "isNullable": true,
          "description": "Display text for checkResult"
        },
        {
          "name": "riskScore",
          "dataType": "integer",
          "isNullable": true,
          "description": "Computed risk score (0-100) based on the KYC check outcome"
        },
        {
          "name": "verifiedBy",
          "dataType": "string",
          "isNullable": true,
          "description": "Name or ID of the person/system that performed the verification"
        },
        {
          "name": "notes",
          "dataType": "string",
          "isNullable": true,
          "description": "Additional notes or comments about the KYC check"
        },
        {
          "name": "integrationKey",
          "dataType": "string",
          "isNullable": true,
          "description": "Integration key for cross-system identification"
        }
      ]
    }
  ]
}
