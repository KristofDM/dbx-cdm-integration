{
  "jsonSchemaSemanticVersion": "1.5.0",
  "imports": [
    { "corpusPath": "cdm:/foundations.4.3.cdm.json" }
  ],
  "definitions": [
    {
      "entityName": "Account",
      "exhibitsTraits": [
        {
          "traitReference": "is.CDM.entityVersion",
          "arguments": [{ "name": "versionNumber", "value": "1.0" }]
        }
      ],
      "description": "A business or customer account at the bank. Source: FinancialServicesCommonDataModel/Account.1.0.cdm.json",
      "sourceName": "msfsi_account",
      "hasAttributes": [
        {
          "name": "accountId",
          "dataType": "entityId",
          "isNullable": false,
          "isPrimaryKey": true,
          "description": "Unique identifier for the account"
        },
        {
          "name": "branchId",
          "dataType": "entityId",
          "isNullable": true,
          "description": "Foreign key to the Branch where the account is held"
        },
        {
          "name": "primaryContactId",
          "dataType": "entityId",
          "isNullable": true,
          "description": "Foreign key to the primary contact/customer"
        },
        {
          "name": "createdOn",
          "dataType": "dateTime",
          "isNullable": true,
          "description": "Date and time when the record was created"
        },
        {
          "name": "modifiedOn",
          "dataType": "dateTime",
          "isNullable": true,
          "description": "Date and time when the record was last modified"
        },
        {
          "name": "statecode",
          "dataType": "listLookup",
          "isNullable": false,
          "description": "Status of the account. Values: 0=Active, 1=Inactive",
          "optionSet": {
            "values": [
              { "value": 0, "displayText": "Active" },
              { "value": 1, "displayText": "Inactive" }
            ]
          }
        },
        {
          "name": "statecode_display",
          "dataType": "string",
          "isNullable": true,
          "description": "Display text for statecode"
        },
        {
          "name": "statuscode",
          "dataType": "listLookup",
          "isNullable": true,
          "description": "Reason for the status",
          "optionSet": {
            "values": [
              { "value": 1, "displayText": "Active" },
              { "value": 2, "displayText": "Inactive" }
            ]
          }
        },
        {
          "name": "statuscode_display",
          "dataType": "string",
          "isNullable": true,
          "description": "Display text for statuscode"
        },
        {
          "name": "name",
          "dataType": "string",
          "isNullable": true,
          "description": "Account name"
        },
        {
          "name": "accountNumber",
          "dataType": "string",
          "isNullable": true,
          "description": "Account number (e.g., IBAN)"
        },
        {
          "name": "accountType",
          "dataType": "string",
          "isNullable": true,
          "description": "Type of account (Checking, Savings, Business, Joint, Student)"
        },
        {
          "name": "joinDate",
          "dataType": "dateTime",
          "isNullable": true,
          "description": "Date the account was opened"
        },
        {
          "name": "tenureYears",
          "dataType": "decimal",
          "isNullable": true,
          "description": "Number of years the account has been open",
          "traits": [
            {
              "traitReference": "is.dataFormat.numeric.shaped",
              "arguments": [
                { "name": "precision", "value": "18" },
                { "name": "scale", "value": "2" }
              ]
            }
          ]
        },
        {
          "name": "integrationKey",
          "dataType": "string",
          "isNullable": true,
          "description": "Integration key for cross-system identification"
        }
      ]
    }
  ]
}
